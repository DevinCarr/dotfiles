# Easier navigation: .., ..., ...., ....., ~ and -
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ~="cd ~"
alias -- -="cd -"

# Shortcuts
alias d="cd ~/Downloads"
alias g="cd ~/Documents/github"
alias h="history"
alias gs="git status"
alias gb="go build"

alias w="cd $HOME/Documents/github/CSCE313"
alias updateweb="appcfg.py --oauth2 update $HOME/Documents/github/website"
